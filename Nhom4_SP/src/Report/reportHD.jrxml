	<queryString>
		<![CDATA[SELECT ChiTietDonHang.*, DonHang.*, KhachHang.*, SanPham.*, TaiKhoan.*
FROM     ChiTietDonHang INNER JOIN
                  DonHang ON ChiTietDonHang.maDH = DonHang.maDH INNER JOIN
                  KhachHang ON DonHang.maKH = KhachHang.maKH INNER JOIN
                  SanPham ON ChiTietDonHang.maSP = SanPham.maSP INNER JOIN
                  TaiKhoan ON KhachHang.maKH = TaiKhoan.maKH
WHERE DonHang.maDH = $P{sMaDH}]]>
	</queryString>
